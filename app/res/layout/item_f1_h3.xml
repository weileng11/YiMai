<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_gravity="center"
	android:orientation="vertical"
	android:padding="@dimen/dp5"
	>
	
	<km.view.GifImageView
		android:id="@+id/f1_h3_vip_go"
		android:layout_width="match_parent"
		android:layout_height="@dimen/dp90"
		android:layout_margin="@dimen/dp5"
		android:src="@mipmap/vip_go"
		/>
	
	<android.support.v7.widget.CardView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="@dimen/dp5"
		app:cardBackgroundColor="@color/colorBackground"
		app:cardCornerRadius="@dimen/card_corner"
		app:cardElevation="@dimen/card_elevation"
		>
		
		<km.view.KmRecyclerView
			android:id="@+id/f1_h3_box"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="@dimen/_dp1"
			app:layoutManager="@string/GridLayoutManager"
			app:spanCount="2"
			tools:layout_height="@dimen/dp310"
			tools:listitem="@layout/item_good"
			/>
		
	</android.support.v7.widget.CardView>
	
</LinearLayout>